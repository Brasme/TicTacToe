    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (mingw gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/ticTacToe/Debug/ticTacToe3D.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "miDebuggerPath": "/usr/bin/gdb",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Launch (vs)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/ticTacToe/Debug/ticTacToe3D.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true            
        },
        {
            "name": "Debug",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "D:/Source/TicTacToe3D/build/src/ticTacToeTest/Debug/ticTacToe3DTest.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true
        }
    ]
}